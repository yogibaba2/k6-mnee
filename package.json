{
  "name": "k6-load-test-project",
  "version": "1.0.0",
  "description": "A project for load testing using K6",
  "main": "src/sample-test.js",
  "scripts": {
    "test": "./k6 run -e ENV=stg src/sample-test-v2.js",
    "cloud-test": "./k6 cloud run -e ENV=stg src/sample-test-v2.js",
    "cloud-local-test": "./k6 cloud run -e ENV=stg --local-execution  src/sample-test-v2.js",
    "test-prometheus": "./k6 run -e ENV=stg -o experimental-prometheus-rw src/sample-test.js",
    "set-report-true": "$Env:K6_WEB_DASHBOARD=\"true\"; $Env:K6_WEB_DASHBOARD_EXPORT=\"html-report.html\"",
    "set-prometheus": "$Env:K6_PROMETHEUS_RW_SERVER_URL=\"http://localhost:9090/api/v1/write\"",
    "set-prometheus-metroices": "$Env:K6_PROMETHEUS_RW_TREND_STATS=\"count,sum,min,max,avg,med,p(90),p(95),p(99)\""
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/atob": "^2.1.4",
    "@types/k6": "^1.1.1",
    "atob": "^2.1.2"
  }
}
